# Supabase Configuration
# These variables are used for both client and server-side operations
# For production, you may want separate keys with different permission levels
NEXT_PUBLIC_SUPABASE_URL=https://your-project-id.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Application Configuration
# For local development, use http://localhost:3000
# For production, use your deployed URL (e.g., https://golf-compete.netlify.app)
NEXT_PUBLIC_SITE_URL=http://localhost:3000
NEXT_PUBLIC_APP_NAME=GolfCompete

# OpenAI
OPENAI_API_KEY=your_openai_api_key_here

# Environment Configuration
# Set to 'development', 'test', or 'production'
NODE_ENV=development

# Optional: Analytics (if needed later)
# NEXT_PUBLIC_ANALYTICS_ID=your_analytics_id_here

# Optional: Storage Configuration
# NEXT_PUBLIC_STORAGE_BUCKET=your_storage_bucket_name_here

# Required Configuration
ANTHROPIC_API_KEY=your-anthropic-api-key-here

# Optional Configuration
MODEL=claude-3-7-sonnet-20250219
MAX_TOKENS=4000
TEMPERATURE=0.7
DEBUG=false
LOG_LEVEL=info
DEFAULT_SUBTASKS=3
DEFAULT_PRIORITY=medium
PROJECT_NAME=Golf Compete API Conversion
PROJECT_VERSION=1.0.0